<view class="home-banner__container">
  <view class="body body-space">
    <view class="page-section">
      <swiper indicator-dots="{{true}}" indicator-color="rgba(0, 0, 0, .3)" indicator-active-color="rgba(0, 0, 0, 1)" current="{{0}}" current-item-id="" previous-margin="" next-margin="" display-multiple-items="{{1}}"  autoplay="{{true}}" interval="{{2500}}" circular="{{true}}"  duration="{{500}}" bindanimationfinish="animationFinish" bindtransition="transition" >
        <block tt:for="{{background}}">
          <swiper-item>
            <image class="image" src="/img/common/coming-soon.jpg" mode="aspectFill" />
          </swiper-item>
        </block>
      </swiper>
    </view>
  </view>
</view>